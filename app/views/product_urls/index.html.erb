<p id="notice"><%= notice %></p>

<h1>Product Urls</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Blog</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_urls.each do |product_url| %>
      <tr>
        <td><%= product_url.price %></td>
        <td><%= product_url.blog_id %></td>
        <td><%= link_to 'Show', product_url %></td>
        <td><%= link_to 'Edit', edit_product_url_path(product_url) %></td>
        <td><%= link_to 'Destroy', product_url, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product Url', new_product_url_path %>
